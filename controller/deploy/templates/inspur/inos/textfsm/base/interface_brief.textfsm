name: interfaceBriefTextFSM
template: |
  Value INTERFACE (\S+)
  Value NAME (.+?)
  Value STATUS (\w+)
  Value VLAN (\S+)
  Value DUPLEX (\S+)
  Value SPEED (\S+)
  Value TYPE (\S+)
  
  Start
    ^${INTERFACE}\s+${NAME}\s+${STATUS}\s+${VLAN}\s+${DUPLEX}\s+${SPEED}\s+${TYPE} -> Record

inputField: interfaceBrief
rowFields:
  - INTERFACE
  - NAME
  - STATUS
  - VLAN
  - DUPLEX
  - SPEED
  - TYPE